{{>layouts/header}}

    <table class="table">
        <thread>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Title</th>
                <th scope="col">Content</th>
            </tr>
        </thread>
        <tbody>
            {{#articleList}}
